# Remove debug tool
rm /usr/share/packages/check-dependencies.sh

# Export paths
export PATH="${PATH}"':/usr/bin'

# Install packages
echo -ne " ${BLUE}[${SEABLUE}::${BLUE}] ${GREEN}Installing packages:${NOCOLOR}${WHITE} "
/usr/bin/package-installer
echo -e "$NOCOLOR"

# Start packages daemons
echo -ne " ${BLUE}[${SEABLUE}::${BLUE}] ${GREEN}Starting daemons:${NOCOLOR}${WHITE} "
/usr/bin/package-starter
echo -e "$NOCOLOR"
