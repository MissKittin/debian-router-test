# Parse init cmdline
echo -e " ${BLUE}[${SEABLUE}::${BLUE}] ${GREEN}Parsing kernel command line${NOCOLOR}"
for x in $(cat /proc/cmdline); do
	case $x in
		hostname=*)
			hostname="${x#hostname=}"
		;;
		nodhcp)
			nodhcp=yes
		;;
		network_ip=*)
			aip="${x#network_ip=}"
		;;
		start_ip=*)
			bip="${x#start_ip=}"
		;;
		netmask=*)
			netmask="${x#netmask=}"
		;;
	esac
done
